{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "swift: Build Debug Chordinate",
            "type": "shell",
            "command": "swift build",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
        },
        {
            "label": "swift: Build Release Chordinate",
            "type": "shell",
            "command": "swift build -c release",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build",
            "problemMatcher": []
        }
    ]
}
